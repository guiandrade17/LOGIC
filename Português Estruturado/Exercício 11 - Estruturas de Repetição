1)ALGORITMO_TABUADA_MULTIPLICACAO_5
VAR contador : INTEIRO
INICIO
| ESCREVA ("INFORME UM NÚMERO PARA SABER SUA TABUADA DE ADIÇÃO")
| LEIA(numero)
| contador ← 1
| ENQUANTO contador <= 10 FAÇA
|  ESCREVA "5 x", contador, " = ", 5 * contador
|  contador ← contador + 1
| FIM_ENQUANTO
FIM

2)ALGORITMO_DOBRO_ATE_DIGITAR_ZERO
VAR numero  :  INTEIRO
INICIO
| LEIA(numero)
| ENQUANTO numero = 0 FAÇA
|     ESCREVA "Dobro de", numero, " = ", numero * 2
|     LEIA(numero)
| FIM_ENQUANTO
FIM

3)ALGORITMO_SOMA_ATE_DIGITAR_ZERO
VAR numero, soma  :  INTEIRO
INICIO
| soma ← 0
| LEIA(numero)
| ENQUANTO numero != 0 FAÇA
|    soma ← soma + numero
|    LEIA(numero)
| FIM_ENQUANTO
FIM

4)ALGORITMO_SOMA_10_NUMEROS
VAR contador, numero, soma  :  INTEIRO
INICIO
| contador ← 1
| soma ← 0
| ENQUANTO contador <= 10 FAÇA
|    ESCREVA("Digite o", contador, "numero: ")
|    LEIA(numero)
|    soma ← soma + numero
|    contador ← contador + 1
| FIM_ENQUANTO
FIM

5)ALGORITMO_MEDIA_PARES_ATE_ZERO
VAR numero, soma, quantidade  :  INTEIRO
media :  REAL
INICIO
| soma ← 0
| quantidade ← 0
| LEIA(numero)
| ENQUANTO numero != 0 FAÇA
|    SE numero % 2 = 0 ENTÃO
|       soma ← soma + numero
|       quantidade ← quantidade + 1
| FIM-SE
| LEIA(numero)
| FIM_ENQUANTO
| SE quantidade > 0 ENTÃO
|    media ← soma / quantidade
|    ESCREVA "Média dos pares = ", media
| SENAO
|    ESCREVA "Nenhum número par foi digitado."
| FIM-SE
FIM
